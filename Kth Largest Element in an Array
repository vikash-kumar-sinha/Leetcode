#include<queue>
class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
    //    sort(nums.begin(),nums.end());
    //    return nums[nums.size()-k]; 
    priority_queue<int>p;
    for(int i=0;i<nums.size();i++)
    p.push(nums[i]);


    for(int i=0;i<k-1;i++)
     p.pop();

     return p.top();
    }

    
};
